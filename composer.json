{
    "name": "thetheago/laravel-seeder-versioning",
    "description": "A Laravel package that provides version control for database seeders, similar to Laravel's migration system. This package tracks which seeders have been run and only executes seeders that have been modified by monitoring class content changes.",
    "type": "library",
    "keywords": ["laravel", "seeders", "versioning", "database"],
    "license": "MIT",
    "scripts": {
        "test": [
            "vendor/bin/phpunit"
        ],
        "coverage": [
            "vendor/bin/phpunit tests/ --coverage-html=coverage-html"
        ]
    },
    "autoload": {
        "psr-4": {
            "Thetheago\\SeederVersioning\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Thetheago\\SeederVersioning\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Thiago",
            "email": "theago.dev@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Thetheago\\SeederVersioning\\SeederVersioningServiceProvider"
            ],
            "aliases": {
                "SeederVersioning": "Thetheago\\SeederVersioning\\Facades\\SeederVersioning"
            }
        }
    },
    "require": {
        "php": ">=8.3",
        "illuminate/support": "^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0",
        "phpunit/phpunit": "^12.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
